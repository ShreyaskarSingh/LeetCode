  void rotate(vector<int>& nums, int k) {
        int temp,prev=nums[0],temp2,j;
        
        for(int i=0;i<k;i++){
            
            nums[0]=nums[nums.size()-1];
            
            for( j=0;j<nums.size()-1;j++){
             
               temp2 = nums[j+1];
               nums[j+1] = prev;
               prev = temp2 ;
                
            }   
        }
    }
